{
  "include": [
    "main.js",
    "renderer.js",
    "app/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "content"
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowJs": true
  }
}